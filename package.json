{
  "name": "@pikokr/command.ts",
  "description": "Discord.js command framework for typescript.",
  "version": "5.3.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@discordjs/ts-docgen": "^0.4.1",
    "@swc/core": "1.2.223",
    "@types/chalk": "2.2.0",
    "@types/lodash": "4.14.184",
    "all-contributors-cli": "6.20.0",
    "discord.js": "14.3.0",
    "dotenv": "16.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "tslog": "^3.3.4",
    "tsup": "6.2.1",
    "typedoc": "^0.23.13",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@types/node": "^16.0.0",
    "chalk": "4.1.2",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "walk-sync": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pikokr/command.ts"
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "rimraf dist && tsup-node",
    "docs:dev": "typedoc",
    "docs:build": "typedoc",
    "docs": "typedoc --json docs/typedoc-out.json --tsconfig tsconfig.prod.json src/index.ts && ts-node scripts/docs",
    "test": "yarn ts-node --swc test"
  },
  "peerDependencies": {
    "discord.js": "*",
    "tslog": "*"
  }
}
