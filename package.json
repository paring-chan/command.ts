{
  "name": "@pikokr/command.ts",
  "description": "Discord.js command framework for typescript.",
  "packageManager": "pnpm@8.3.1",
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@discordjs/ts-docgen": "^0.4.1",
    "@swc/core": "1.2.223",
    "@types/lodash": "4.14.184",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "all-contributors-cli": "6.20.0",
    "discord.js": "14.8.0",
    "dotenv": "16.0.1",
    "eslint": "latest",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "semantic-release": "^21.0.0",
    "ts-node": "10.9.1",
    "tslog": "^4.7.1",
    "tsup": "6.2.1",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/node": "^16.0.0",
    "chalk": "4.1.2",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "walk-sync": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pikokr/command.ts"
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "rimraf dist && tsup-node",
    "docs:dev": "typedoc",
    "docs:build": "typedoc",
    "docs": "typedoc --json docs/typedoc-out.json --tsconfig tsconfig.prod.json src/index.ts && ts-node scripts/docs",
    "test": "yarn ts-node --swc test",
    "lint": "eslint --ignore-path .gitignore .",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "discord.js": "14.8",
    "tslog": "*"
  },
  "release": {
    "branches": [
      "stable",
      {
        "name": "dev",
        "prerelease": true
      }
    ]
  }
}
