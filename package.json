{
  "name": "@pikokr/command.ts",
  "description": "Discord.js command framework for typescript.",
  "version": "5.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@discordjs/ts-docgen": "0.4.1",
    "@swc/core": "1.2.218",
    "@types/chalk": "2.2.0",
    "@types/lodash": "4.14.182",
    "all-contributors-cli": "6.20.0",
    "discord.js": "14.0.1",
    "dotenv": "16.0.1",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "tsup": "5.11.12",
    "typedoc": "0.22.11",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@types/node": "^14.14.37",
    "chalk": "4.1.2",
    "discord-api-types": "^0.23.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.3.1",
    "walk-sync": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pikokr/command.ts"
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "tsup-node",
    "docs:dev": "typedoc",
    "docs:build": "typedoc",
    "docs": "typedoc --json docs/typedoc-out.json --tsconfig tsconfig.prod.json src/index.ts && ts-node scripts/docs",
    "test": "yarn ts-node --swc test"
  },
  "peerDependencies": {
    "discord.js": "^14.0.1"
  }
